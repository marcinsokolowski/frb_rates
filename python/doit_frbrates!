#!/bin/bash


python /home/msok/github/frb_rates/python/plot_frb_rates.py --grid --legend_with_curves --fluence_threshold=580 > out 2>&1

# the numbers are as in the caption to figure 3 in the paper:
# 10ms :
# 1 channel : 
cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==25924){print $0;}}' > eda2_1channel_2000ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_1channel_2000ms.txt > eda2_1channel_2000ms.rate

cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==18331){print $0;}}' > eda2_1channel_1000ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_1channel_1000ms.txt > eda2_1channel_1000ms.rate

cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==5797){print $0;}}' > eda2_1channel_100ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_1channel_100ms.txt > eda2_1channel_100ms.rate

