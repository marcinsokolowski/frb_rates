python ~/github/frb_rates/python/rms_txtfile.py eda2_1channel_1ms.txt > eda2_1channel_1ms.rate

# 13 channels :
cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==7906){print $0;}}' > eda2_13channel_2000ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_13channel_2000ms.txt > eda2_13channel_2000ms.rate

