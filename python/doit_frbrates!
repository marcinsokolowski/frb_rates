python ~/github/frb_rates/python/rms_txtfile.py eda2_13channel_1000ms.txt > eda2_13channel_1000ms.rate

cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==1768){print $0;}}' > eda2_13channel_100ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_13channel_100ms.txt > eda2_13channel_100ms.rate

cat chime.txt GBT_Parent_et_al_2020.txt  Parkes_Bhandari_et_al_2018.txt  shannon_et_al_2018.txt  UTMOST_Farash_et_al_2019.txt | awk '{if($1==559){print $0;}}' > eda2_13channel_10ms.txt
python ~/github/frb_rates/python/rms_txtfile.py eda2_13channel_10ms.txt > eda2_13channel_10ms.rate

